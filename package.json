{
  "name": "dockerfarm-backend",
  "version": "0.0.1",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --inspect -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^5.0.1",
    "eslint-plugin-async-await": "^0.0.0",
    "express": "^4.13.3",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.8.0",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^4.37.5"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^4.19.1",
    "nodemon": "^1.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DockerFarm/DockerFarm-backend.git"
  },
  "keywords": [
    "docker"
  ],
  "author": "y0c",
  "bugs": {
    "url": "https://github.com/DockerFarm/DockerFarm-backend/issues"
  },
  "homepage": "https://github.com/DockerFarm/DockerFarm-backend#readme"
}
