{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-console": "off",
        "no-empty" : "off"
    }
}